"build": {
  "builder": "@angular-devkit/build-angular:application",
  "options": {
    "outputPath": "dist/remove-bg-app",
    "index": "src/index.html",
    "main": "src/main.ts",
    "polyfills": [
      "zone.js"
    ],
    "tsConfig": "tsconfig.app.json",
    "inlineStyleLanguage": "scss",
    "assets": [
      {
        "glob": "**/*",
        "input": "public",
        "output": "/public"
      }
    ],
    "styles": [
      "src/styles.scss"
    ],
    "scripts": [],
    "baseHref": "/"
  },
  "configurations": {
    "production": {
      "budgets": [
        {
          "type": "initial",
          "maximumWarning": "500kB",
          "maximumError": "1MB"
        },
        {
          "type": "anyComponentStyle",
          "maximumWarning": "4kB",
          "maximumError": "8kB"
        }
      ],
      "outputHashing": "all",
      "baseHref": "/"
    },
    "development": {
      "optimization": false,
      "extractLicenses": false,
      "sourceMap": true,
      "baseHref": "/"
    }
  },
  "defaultConfiguration": "production"
}
